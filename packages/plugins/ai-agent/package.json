{
  "name": "@fetchmax/plugin-ai-agent",
  "version": "1.0.2",
  "description": "AI Agent connection plugin for FetchMax - unified interface for OpenAI, Anthropic, DeepSeek, and more",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },

  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "keywords": [
    "fetchmax",
    "plugin",
    "ai",
    "llm",
    "openai",
    "anthropic",
    "deepseek",
    "gemini",
    "ollama",
    "chatgpt",
    "claude",
    "ai-agent",
    "cost-tracking",
    "rate-limiting"
  ],
  "author": "FetchMax Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zernabhussain/fetchmax.git",
    "directory": "packages/plugins/ai-agent"
  },
  "bugs": {
    "url": "https://github.com/zernabhussain/fetchmax/issues"
  },
  "homepage": "https://github.com/zernabhussain/fetchmax#readme",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@fetchmax/core": "^1.0.0"
  },
  "devDependencies": {
    "@fetchmax/core": "*",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
