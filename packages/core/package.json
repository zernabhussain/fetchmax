{
  "name": "@fetchmax/core",
  "version": "1.0.2",
  "description": "Modern, lightweight, and extensible HTTP client for JavaScript/TypeScript. Built on native Fetch API with powerful plugin system. Works in Node.js, Deno, Bun, and browsers.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "keywords": [
    "http",
    "https",
    "fetch",
    "client",
    "ajax",
    "request",
    "axios",
    "ky",
    "got",
    "universal",
    "isomorphic",
    "typescript",
    "plugin",
    "plugins",
    "modular",
    "extensible",
    "lightweight",
    "api",
    "rest",
    "graphql",
    "browser",
    "node",
    "deno",
    "bun",
    "retry",
    "cache",
    "timeout",
    "interceptors",
    "middleware"
  ],
  "author": "FetchMax Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zernabhussain/fetchmax.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/zernabhussain/fetchmax/issues"
  },
  "homepage": "https://github.com/zernabhussain/fetchmax#readme",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
